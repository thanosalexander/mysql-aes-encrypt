{
    "name": "redsd/aesencrypt",
    "type": "library",
    "description": "Laravel MySql AES Encrypt/Decrypt",
    "keywords": ["laravel","mysql", "aes", "encrypt"],
    "homepage": "https://github.com/redsd/mysql-aes-encrypt",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Eric van Eldik",
            "email": "ericvaneldik@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/config": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "redsd\\AESEncrypt\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"

        },
        "laravel": {
            "providers": [
                "redsd\\AESEncrypt\\Database\\DatabaseServiceProviderEncrypt"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
